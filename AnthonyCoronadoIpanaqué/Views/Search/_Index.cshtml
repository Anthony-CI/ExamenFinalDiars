
@{
    ViewData["Title"] = "_Index";
    Layout = null;
}

@foreach (var item in Model)
{
    <div class="card">
        <h3 class="card-header"><a class="btn btn-secondary" href="/Nota/VerDetalle?id=@item.Id">@item.Titulo</a></h3>
    </div>
}
<hr />
<strong>Tags</strong>
